{
  "name": "root",
  "private": true,
  "workspaces": [
    "android-docs",
    "cs-guide",
    "git-tutor",
    "think-in-java",
    "node-tutor",
    "wangdoc",
    "cg-tutor",
    "childhood",
    "baike",
    "flutter-docs",
    "CS-Notes",
    "hope-demo"
  ],
  "devDependencies": {
    "lerna": "latest",
    "cpx2": "latest"
  },
  "scripts": {
    "build": "lerna run docs:build --parallel",
    "dev": "lerna run docs:dev --parallel",
    "cs-guide": "yarn workspace cs-guide docs:dev -p 9090",
    "cs-guide:build": "yarn workspace cs-guide docs:build",
    "android": "yarn workspace android-docs docs:dev",
    "android:build": "yarn workspace android-docs docs:build",
    "git": "yarn workspace git-tutor docs:dev",
    "git:build": "yarn workspace git-tutor docs:build",
    "node:dev": "yarn workspace node-tutor docs:dev",
    "node:build": "yarn workspace node-tutor docs:build",
    "think": "yarn workspace think-in-java docs:dev",
    "think:build": "yarn workspace think-in-java docs:build",
    "wangdoc": "yarn workspace wangdoc docs:dev",
    "wangdoc:build": "yarn workspace wangdoc docs:build",
    "hope": "yarn workspace hope-demo docs:dev",
    "child": "yarn workspace childhood docs:dev",
    "cg": "yarn workspace cg-tutor docs:dev",
    "cg:build": "yarn workspace cg-tutor docs:build",
    "baike": "yarn workspace baike docs:dev",
    "notes": "yarn workspace CS-Notes docs:dev",
    "notes:build": "yarn workspace CS-Notes docs:build",
    "flu:dev": "yarn workspace flutter-docs docs:dev",
    "flu:build": "yarn workspace flutter-docs docs:build"
  }
}
